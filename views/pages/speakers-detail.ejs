<div class="container mt-4">
  <div class="row">
    <div class="col-md-8">
      <article class="speakerslist maincontent">
        <h1 class="speakerslist-title"><%=artist.title%></h1>
        <div class="speakerslist-name">
          with
          <a href="/artists/<%=artist.shortname%>"><%=artist.name%></a>
        </div>
        <p class="speakerslist-info mt-2">
          <a href="/artists/<%=artist.shortname%>"> </a>
          <img
            class="speakerslist-img img-fluid"
            src="/images/speakers/<%=artist.shortname%>.jpg"
            alt="Photo of <%=artist.name%>"
          />
        </p>
        <%-artist.description%>
        <p></p>
      </article>
    </div>
    <aside class="col-md-4"><%- include ('./partials/product') %></aside>
  </div>
</div>
